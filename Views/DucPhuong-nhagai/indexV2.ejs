    <script src="/QuyLinh/lib/snow/snowfall.jquery.min.js"></script>
    <script>
    $(document).snowfall({ image: "/QuyLinh/img/heart.png", minSize: 10, maxSize: 32, flakeCount: 20, minSpeed: 3, maxSpeed: 5 });

    function join(e) {
        e.preventDefault()
        const name = $('#name').val()
        const phoneNumber = $('#phone').val()
        console.log(name, phoneNumber);
        $.ajax({
            url: '/DucPhuong/invite',
            method: 'POST',
            data: {
                name: name,
                phoneNumber: phoneNumber,
                sex: 'female'
            },
            success: res => {
                swal("Thông báo", res.message, res.success == 0 ? "error" : "success");

            }
        })
    }
    </script>
